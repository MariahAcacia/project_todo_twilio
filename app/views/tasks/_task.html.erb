<tbody>
  <% @tasks.each do |task| %>
    <tr>
      <td>
        <%= task.description %>
      </td>
      <td>
        <%= task.complete_by_date %>
      </td>
      <td>
        <%= link_to "See Details", task_path(task) %>
      </td>
      <td>
        <%= link_to "Edit", edit_task_path(task) %> 
      </td>
      <td>
        <%= link_to "Delete", task_path(task), method: :delete, data: {confirm: "Are yous sure you want to delete this task?"} %>
      </td>
    </tr>
  <% end %>

</tbody>
